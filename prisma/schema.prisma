generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model articles {
  id      Int    @id @default(autoincrement())
  title   String
  content String
}

model users {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}
